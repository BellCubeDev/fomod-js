<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://qconsulting.ca/fo3/ModConfig5.0.xsd">
    <moduleName>
        Lux
    </moduleName>
    <moduleImage path="fomod\Screenshots\Header 01 LUX.jpg"/>
    <requiredInstallFiles>
        <folder source="00 Data\Lux - Main Files/"/>
    </requiredInstallFiles>
    <installSteps order="Ascending">
        <installStep name="Main files">
            <optionalFileGroups order="Explicit">
                <group name="Lux optional effects" type="SelectAny">
                    <plugins order="Explicit">
                        <plugin name="Beams - PARTICLE without effect lighting">
                            <description>
                                Optional reworked beams, balanced emissive strength and dimmer visuals. Most meshes can be now tweaked in the PARTICLE section of your ENB but can't receive any light from surrounding light sources as a side effect. CHOOSE only one Beam option. (Author's choice)
                            </description>
                            <image path="fomod\Screenshots\_Beams.jpg"/>
                            <conditionFlags>
                                <flag name="OPTION_Beams___PARTICLE_without_effect_lighting--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Fx\Beams\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Beams - Effect lighting without PARTICLE">
                            <description>
                                Optional reworked beams, less opacity, beams are affected by surrounding lighting but can't be tweaked with ENB's Particle settings as a side effect. CHOOSE only one beam option.
                            </description>
                            <image path="fomod\Screenshots\_Beams.jpg"/>
                            <conditionFlags>
                                <flag name="OPTION_Beams___Effect_lighting_without_PARTICLE--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Fx\Beams (no ENB)\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Mists and fogs - PARTICLE without effect lighting">
                            <description>
                                Optional reworked mists and fogs, balanced emissive strength and dimmer visuals. Most meshes can be now tweaked in the PARTICLE section of your ENB but can't receive any light from surrounding light sources as a side effect. CHOOSE only one mists and fogs option.
                            </description>
                            <image path="fomod\Screenshots\_Mist.jpg"/>
                            <conditionFlags>
                                <flag name="OPTION_Mists_and_fogs___PARTICLE_without_effect_lighting--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Fx\Fogs\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Mists and fogs - Effect lighting without PARTICLE">
                            <description>
                                Optional reworked mists and fogs, less opacity, beams are affected by surrounding lighting but can't be tweaked with ENB's Particle settings as a side effect. CHOOSE only one mists and fogs option. (Author's choice)
                            </description>
                            <image path="fomod\Screenshots\_Mist.jpg"/>
                            <conditionFlags>
                                <flag name="OPTION_Mists_and_fogs___Effect_lighting_without_PARTICLE--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Fx\Fogs (no ENB)\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Webs">
                            <description>
                                Optional reworked spider webs meshes so they're dimmer overall. Not useful if using T4's mod, his version is a way better option.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Webs--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Fx\Webs\meshes/" destination="meshes/"/>
                                <folder source="00 Data\Lux - Fx\Lux - RCI web corpses\meshes/" destination="meshes/"/>
                                <folder source="00 Data\Lux - Fx\Lux - RCI web corpses\textures/" destination="textures/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
                <group name="Lux fixed Ice Cave meshes" type="SelectAtMostOne">
                    <plugins order="Explicit">
                        <plugin name="Fixed Vanilla meshes">
                            <description>
                                I removed backlighting flag and backlighting texture from all ice cave meshes rock part so rock isn't glowing from inside anymor.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Fixed_Vanilla_meshes--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Ice Cave Rock meshes\Vanilla\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Fixed Multilayer Parallax meshes">
                            <description>
                                I removed backlighting flag and backlighting texture from all ice cave meshes rock part so rock isn't glowing from inside anymor, choose this option if using multilayer parallax ice, not compatible with Remastered Ice Caves since that mod is replacing all rock with ice.
                            </description>
                            <image path="fomod\Screenshots\ICE PARALLAX.jpg"/>
                            <conditionFlags>
                                <flag name="OPTION_Fixed_Multilayer_Parallax_meshes--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Ice Cave Rock meshes\Multilayer Parallax\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
                <group name="Lux optimized rugs" type="SelectAtMostOne">
                    <plugins order="Explicit">
                        <plugin name="Vanilla rugs">
                            <description>
                                Choose that option if using vanilla rugs. Meshes are splitted so they're less likely to break because of lighting.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Vanilla_rugs--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Rugs\Vanilla\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="RUGNAROK rugs">
                            <description>
                                Choose that option if using RUGNAROK rugs. Meshes are splitted so they're less likely to break because of lighting.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_RUGNAROK_rugs--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Rugs\Rugnarok\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Detailed rugs">
                            <description>
                                Choose that option if using Detailed rugs. Meshes are splitted so they're less likely to break because of lighting.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Detailed_rugs--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Rugs\Detailed Rugs\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
                <group name="Lux optimized Pelts" type="SelectAtMostOne">
                    <plugins order="Explicit">
                        <plugin name="Vanilla pelts">
                            <description>
                                Choose that option if using vanilla pelts only. Meshes are split so they're less likely to break because of lighting.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Vanilla_pelts--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Pelts\Vanilla Meshes\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Vanilla pelts (parallax)">
                            <description>
                                Choose that option if using vanilla pelts with parallax support only. Meshes are split so they're less likely to break because of lighting.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Vanilla_pelts__parallax_--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Pelts\Vanilla Meshes (parallax)\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="PELTAPALOOZA Pelts">
                            <description>
                                Choose that option if using PELTAPALOOZA pelts Full version, it won't work as intended with the Lite version. Meshes are split so they're less likely to break because of lighting.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_PELTAPALOOZA_Pelts--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Pelts\PELTAPALOOZA\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="PELTAPALOOZA Pelts (parallax)">
                            <description>
                                Choose that option if using PELTAPALOOZA pelts Full version with parallax support, it won't work as intended with the Lite version. Meshes are split so they're less likely to break because of lighting.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_PELTAPALOOZA_Pelts__parallax_--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Pelts\PELTAPALOOZA (parallax)\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
                <group name="Lux optimized tables" type="SelectAtMostOne">
                    <plugins order="Explicit">
                        <plugin name="Vanilla">
                            <description>
                                Optimized meshes for vanilla tables.
