                            <conditionFlags>
                                <flag name="OPTION_SD_s_Table_replacer--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Tables\SD's Table Replacer\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
                <group name="Lux optimized chandeliers" type="SelectAny">
                    <plugins order="Explicit">
                        <plugin name="Vanilla chandeliers">
                            <description>
                                Modified meshes for vanilla chandeliers so that they stop casting shadows. Pick one or all options based on mods you're using. May lead to slightly transparent chandeliers in some circumstances.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Vanilla_chandeliers--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Chandeliers\Vanilla\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="SMIM chandeliers">
                            <description>
                                Modified meshes for SMIM chandeliers so that they stop casting shadows. Pick one or all options based on mods you're using (Do not include textures). May lead to slightly transparent chandeliers in some circumstances.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_SMIM_chandeliers--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Chandeliers\SMIM\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="RCI chandeliers">
                            <description>
                                Modified meshes for Ruin Clutters Improved chandeliers so that they stop casting shadows. Pick one or all options based on mods you're using (Do not include textures). May lead to slightly transparent chandeliers in some circumstances.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_RCI_chandeliers--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Chandeliers\Ruins Clutter Improved\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="SD's chandeliers">
                            <description>
                                Modified meshes for SD's chandeliers so that they stop casting shadows. Pick one or all options based on mods you're using (Do not include textures). May lead to slightly transparent chandeliers in some circumstances.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_SD_s_chandeliers--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Chandeliers\SD's Horn Candles\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
                <group name="Lux Embers XD compatibility" type="SelectAny">
                    <plugins order="Explicit">
                        <plugin name="BYOH fireplace">
                            <description>
                                Optimized Hearthfire BYOH fireplace for Embers users.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_BYOH_fireplace--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Embers\Lux - Embers XD\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="EmbersXD new fire effect asset">
                            <description>
                                Option to replace the vanilla flame effect I'm relying on (for burning dwemer lanterns mostly) with EmbersXD own flame texture, do not install that option if you're not specifically using EmbersXD nor its optional fire effects or if you're using the Inferno combo. That option needs you to have those flame textures to work or you'll get purple squares on my flames instead.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_EmbersXD_new_fire_effect_asset--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Embers Fire replacer\meshes/" destination="meshes/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
                <group name="Lux optional Giant Campfires" type="SelectAtMostOne">
                    <plugins order="Explicit">
                        <plugin name="Giant Campfires">
                            <description>
                                Modified Giant Campfire with shadow casting removal to prevent weird shadows. ENB light compatibe. Use EmbersXD version instead if needed, both will conflict.
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Giant_Campfires--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Fire\Giant Campfire\meshes/" destination="meshes/"/>
                                <folder source="00 Data\Fire\Giant Campfire\textures/" destination="textures/"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
                <group name="Lux optimized meshes" type="SelectExactlyOne">
                    <plugins order="Explicit">
                        <plugin name="Vanilla">
                            <description>
                                Automatically selected vanilla meshes, optimized for lighting. Those meshes are mandatory to make the mod work as intended.
                            </description>
                            <conditionFlags>
                                <flag name="OptimizedMeshes">
                                    On
                                </flag>
                                <flag name="ParallaxMeshes">
                                    Off
                                </flag>
                                <flag name="OPTION_Vanilla--2">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Parallax">
                            <description>
                                Automatically selected parallax meshes, optimized for lighting. Those meshes are mandatory to make the mod work as intended.
                            </description>
                            <conditionFlags>
                                <flag name="OptimizedMeshes">
                                    On
                                </flag>
                                <flag name="ParallaxMeshes">
                                    On
                                </flag>
                                <flag name="OPTION_Parallax--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="None">
                            <description>
                                Optimized meshes installation will be skipped entirely, only use that option when updating plugins only. Merge that update in your mod organizer to keep your already installed meshes.
                            </description>
                            <conditionFlags>
                                <flag name="OptimizedMeshes">
                                    Off
                                </flag>
                                <flag name="OPTION_None--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                    </plugins>
                </group>
            </optionalFileGroups>
        </installStep>
