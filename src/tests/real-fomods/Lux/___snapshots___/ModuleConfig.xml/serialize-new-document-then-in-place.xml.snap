                            </description>
                            <image path="fomod\Screenshots\Lux Windows.jpg"/>
                            <conditionFlags>
                                <flag name="OPTION_Default_vanilla_models--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <folder source="00 Data\Lux - Window models\Lux - Optimized flat windows\meshes/" destination="meshes/"/>
