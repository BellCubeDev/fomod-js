        <installStep name="Fixes">
            <optionalFileGroups order="Explicit">
                <group name="Landscape fixes" type="SelectAny">
                    <plugins order="Explicit">
                        <plugin name="No Grass In Caves">
                            <description>
                                No Grass in Caves patch so that grass isn't generated in reworked worldspaces such as underground caves, has no dependency and works as a no grass in caves mod replacer fully compatible with Lux (ESPFE)
                            </description>
                            <image path="fomod\Screenshots\NoWeed.jpeg"/>
                            <conditionFlags>
                                <flag name="OPTION_No_Grass_In_Caves--1">
                                    OPTION_SELECTED
                                </flag>
                            </conditionFlags>
                            <files>
                                <file source="00 Data\Lux - No grass in caves patch.esp" destination="Lux - No grass in caves patch.esp"/>
                            </files>
                            <typeDescriptor>
                                <type name="Optional"/>
                            </typeDescriptor>
                        </plugin>
                        <plugin name="Landscape and Water fixes">
                            <description>
                                Compatibility patch for Landscape and water fixes, for instance edits to Heartwood Mill (Credits to Cosmic Sloth) (ESPFE)
                            </description>
                            <conditionFlags>
                                <flag name="OPTION_Landscape_and_Water_fixes--1">
                                    OPTION_SELECTED
                                </flag>
